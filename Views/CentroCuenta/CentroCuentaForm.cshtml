<form role="form" id="ccAccountsForm" style="margin-left:15px; margin-right:15px;">
    <div class="col-md-12">
        <input type="hidden" class="form-control" id="isUpdating" name="isUpdating" />
        <input type="hidden" class="form-control" id="COD_CIA" name="COD_CIA" />
        <input type="hidden" class="form-control" id="CENTRO_COSTO" name="CENTRO_COSTO" />

        <table>
            <tr>
                <td colspan="7"><label class="form-label">Cuenta contable:</label></td>
            </tr>
            <tr>
                <td><input type="text" name="CTA_1" id="CTA_1" class="form-control"/></td>
                <td><input type="text" name="CTA_2" id="CTA_2" class="form-control"/></td>
                <td><input type="text" name="CTA_3" id="CTA_3" class="form-control"/></td>
                <td><input type="text" name="CTA_4" id="CTA_4" class="form-control"/></td>
                <td><input type="text" name="CTA_5" id="CTA_5" class="form-control"/></td>
                <td><input type="text" name="CTA_6" id="CTA_6" class="form-control"/></td>
                <td style="width: 30%;"><input type="text" placeholder="Nombre" name="NOMBRE" id="NOMBRE" class="form-control" readonly=""/></td>
            </tr>
        </table>

        <div class="form-check mt-3">
            <input class="form-check-input" type="checkbox" value="1" id="ESTADO" name="ESTADO"/>
            <label class="form-check-label" for="ESTADO">
                Activo
            </label>
        </div>

        <div class="row">
            <div class="col-12" style="margin-top:20px;">
                <button class="btn btn-primary btn-md float-right" id="ccAccountsFormAddButton" type="submit">
                    <i class="fa fa-floppy-o"></i>&nbsp;
                    <span id="ccAccountsFormAddButtonText">Guardar</span>
                </button>
            </div>
        </div>
    </div>
</form>